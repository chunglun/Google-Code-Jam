package com.lu;

import java.io.BufferedReader;
import java.io.BufferedWriter;
import java.io.FileInputStream;
import java.io.FileOutputStream;
import java.io.InputStreamReader;
import java.io.OutputStreamWriter;
import java.util.ArrayList;
import java.util.Arrays;
import java.util.HashMap;
import java.util.List;
import java.util.Map;

public class R0A2014
{
	private String inputFileName = "2014_A_small.in";
	private String outputFileName = "2014_A_small.out";
	
	public static void main(String[] args)
	{
		R0A2014 g = new R0A2014();
		g.start();
	}
	
	private void start()
	{
		FileInputStream fin;
		FileOutputStream fout;
		
		try
		{
			fin = new FileInputStream(inputFileName);
			InputStreamReader inputReader = new InputStreamReader(fin);
			BufferedReader br = new BufferedReader(inputReader);
			
			fout = new FileOutputStream(outputFileName);
			OutputStreamWriter outputWriter = new OutputStreamWriter(fout);
			BufferedWriter bw = new BufferedWriter(outputWriter);
			
			int round = Integer.parseInt(br.readLine());
			
			for(int i = 0; i < round; i++)
			{
				Integer ans1 = Integer.parseInt(br.readLine());
				Map<Integer, String[]> map1 = new HashMap<Integer, String[]>();
				for(int j = 0; j < 4; j++)
				{
					String aLine = br.readLine();
					String[] aLineStr = aLine.split(" ");
					map1.put((j+1), aLineStr);
				}
				Integer ans2 = Integer.parseInt(br.readLine());
				Map<Integer, String[]> map2 = new HashMap<Integer, String[]>();
				for(int j = 0; j < 4; j++)
				{
					String aLine = br.readLine();
					String[] aLineStr = aLine.split(" ");
					map2.put((j+1), aLineStr);
				}
				
				String result = execute(map1, map2, ans1, ans2);
				bw.write("Case #" + (i+1) + ": " + result);
				bw.newLine();
			}
			
			br.close();
			bw.flush();
			bw.close();
		}
		catch(Exception e)
		{
			e.printStackTrace();
		}
	}
	
	private String execute(Map<Integer, String[]> map1, Map<Integer, String[]> map2, Integer ans1, Integer ans2)
	{
		List<String> candidate1 = Arrays.asList(map1.get(ans1));
		List<String> candidate2 = Arrays.asList(map2.get(ans2));
		
		List<String> matchedResult = new ArrayList<String>();
		for(String str : candidate1)
		{
			if(candidate2.contains(str))
			{
				matchedResult.add(str);
			}
		}
		
		if(matchedResult.size() == 1)
		{
			return matchedResult.get(0);
		}
		else if(matchedResult.size() == 0)
		{
			return "Volunteer cheated!";
		}
		else
		{
			return "Bad magician!";
		}
	}
}
